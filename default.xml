<manifest>
  <remote fetch="../seL4" name="seL4"/>
  <remote fetch="../sel4proj" name="sel4proj"/>
  <remote fetch="https://github.com/tass-belgium" name="picotcp" />

  <default remote="seL4" revision="master"/>

  <project name="musllibc.git" path="projects/musllibc" revision="sel4" />
  <project name="seL4.git" path="kernel" />
  <!-- we only use libsel4sync from here -->
  <project name="seL4_libs.git" path="projects/seL4_libs"/>
  <!-- we use tools/elfloader and tools/cmake-tool -->
  <project name="seL4_tools.git" path="tools/seL4">
      <linkfile dest="CMakeLists.txt" src="cmake-tool/default-CMakeLists.txt" />
  </project>
  <project name="aos.git" path="projects/aos" revision="wip" remote="sel4proj">
    <linkfile dest="init-build.sh"  src="init-build.sh"/>
  </project>
  <!-- we use libcpio, libelf, libutils, libethdrivers, liblwip, libplatsupport -->
  <project name="util_libs.git" path="projects/util_libs" branch="alyons/aos-wip"/>
  <project name="picotcp.git"       path="projects/picotcp" remote="picotcp" />
</manifest>


